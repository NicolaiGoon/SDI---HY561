  <rdf:Description rdf:about="http://www.csd.uoc.gr/~hy561/2022/1254/2022/5/UniversityOntology#CS-387">
    <isPrerequisiteOf>
      <GraduateCourse rdf:about="http://www.csd.uoc.gr/~hy561/2022/1254/2022/5/UniversityOntology#CS-567">
